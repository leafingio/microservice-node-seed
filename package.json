{
  "name": "microservice-seed",
  "version": "0.0.0-semantically-released",
  "description": "Microservice seed",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/controllers.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/controllers.test.js",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leafingio/microservice-seed.git"
  },
  "keywords": [
    "microservice",
    "seed"
  ],
  "author": "Albert Parr√≥n <al.parron@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leafingio/microservice-seed/issues"
  },
  "homepage": "https://github.com/leafingio/microservice-seed#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.5",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "semantic-release": "^6.3.2"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
